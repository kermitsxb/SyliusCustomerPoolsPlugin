{
    "name": "kermitsxb/sylius-customer-pools-plugin",
    "type": "sylius-plugin",
    "description": "Sylius plugin for managing customer pools.",
    "keywords": [
        "sylius",
        "sylius-plugin"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "sylius/sylius": "^1.11"
    },
    "require-dev": {
        "sensiolabs/security-checker": "^5.0",
        "sylius-labs/coding-standard": "^v4.1",
        "phpstan/phpstan": "^1.6",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-webmozart-assert": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Tavy315\\SyliusCustomerPoolsPlugin\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
